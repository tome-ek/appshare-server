{
  "name": "appshare-server",
  "version": "0.1.0",
  "description": "Appshare API Server",
  "main": "index.ts",
  "engines": {
    "node": ">=16.4.2"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/index.js",
    "build": "rimraf ./build && cross-env NODE_ENV=production tsc -b",
    "lint": "eslint --ext .ts src",
    "format": "prettier -c src",
    "test": "npm run test:coverage",
    "dev": "cross-env NODE_ENV=development ts-node-dev --transpile-only index.ts",
    "test:unit": "cross-env NODE_ENV=test TS_NODE_PROJECT=test/tsconfig.json mocha --config .mocharc.unit.json test/unit",
    "test:integration": "cross-env NODE_ENV=test TS_NODE_PROJECT=test/tsconfig.json mocha --config .mocharc.integration.json test/integration",
    "test:coverage": "cross-env NODE_ENV=test tsc --sourceMap true --project test/tsconfig.json & cross-env NODE_ENV=test nyc mocha --config .mocharc.integration.json test/**/*.spec.ts"
  },
  "author": "Tomasz Bartkowski",
  "license": "AGPL-v3",
  "dependencies": {
    "@hapi/boom": "^9.1.3",
    "awilix": "^4.3.4",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "camel-case": "^4.1.2",
    "chalk": "^4.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "decompress": "^4.2.1",
    "del": "^6.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-basic-auth": "^1.2.0",
    "express-jsdoc-swagger": "^1.6.6",
    "express-validator": "^6.12.1",
    "firebase-admin": "^9.11.0",
    "http-terminator": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "ms": "^2.0.0",
    "multer": "^1.4.2",
    "nanoid": "^3.1.23",
    "node-fetch": "^2.6.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "pino": "^7.0.0-rc.4",
    "pino-pretty": "^7.0.0",
    "sequelize": "^6.6.5",
    "simple-plist": "^1.1.1",
    "ws": "^8.2.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@swc/core": "^1.2.85",
    "@swc/helpers": "^0.2.13",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.36",
    "@types/chai": "^4.2.21",
    "@types/chai-subset": "^1.3.3",
    "@types/cors": "^2.8.12",
    "@types/decompress": "^4.2.4",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.4.1",
    "@types/node-fetch": "^2.5.12",
    "@types/sequelize": "^4.28.10",
    "@types/sinon-chai": "^3.2.5",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "chai": "^4.3.4",
    "chai-subset": "^1.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^9.1.1",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^2.7.1",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "ts-sinon": "^2.0.1",
    "typescript": "^4.4.2"
  }
}
